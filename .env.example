# CrossFit WOD AI Backend - Development Environment Configuration
# Copy this file to .env.local and replace with actual values
# NEVER commit .env.local to git!

# Server Configuration
PORT=3000
NODE_ENV=development
APP_URL=http://localhost:3000

# AI Provider API Keys (GET FROM TEAM - NEVER COMMIT ACTUAL VALUES)
ANTHROPIC_API_KEY=sk-ant-api03-your_actual_key_here
OPENAI_API_KEY=sk-your_openai_key_here
GEMINI_API_KEY=your_gemini_key_here

# LangChain Configuration (optional - for AI monitoring)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_key_optional
LANGCHAIN_PROJECT=crossfit-wod-ai

# Supabase Configuration (GET FROM TEAM)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_anon_key
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_service_key

# Security (GENERATE UNIQUE VALUES)
JWT_SECRET=your_32_character_random_jwt_secret
API_SECRET_KEY=your_32_character_random_api_key

# CORS Origins (adjust for your frontend)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=300

# Logging
LOG_LEVEL=info

# Optional Services (uncomment if needed)
# REDIS_URL=redis://localhost:6379
# STRIPE_SECRET_KEY=sk_test_your_stripe_key
# STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your_email@gmail.com
# SMTP_PASS=your_app_password